{
  "name": "facu-event",
  "private": true,
  "version": "1.0.0",
  "description": "Sistema para eventos.",
  "author": "Alexander David ",
  "license": "MIT",
  "scripts": {
    "start": "cd facuService && yarn start",
    "install": "cd facuAdmin && yarn install && cd ../facuService && yarn install && cd ..",
    "build": "cd facuAdmin && yarn build",

    "serve": "cd facuService && yarn dev & cd facuAdmin && yarn serve",
    "serve:b": "cd facuService && yarn dev ",
    "serve:f": "cd facuAdmin && yarn serve",
    "install:b": "cd facuService && yarn install",
    "install:f": "cd facuAdmin && yarn install",
    "add:b": "cd facuService && yarn add",
    "add:f": "cd facuAdmin && yarn add",
    "lint": "cd facuAdmin && yarn lint"
  },
  "engines": {
    "node": "^10.16",
    "yarn": "^1.17"
  }
}
